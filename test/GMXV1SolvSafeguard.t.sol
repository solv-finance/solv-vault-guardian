pragma solidity 0.8.21;

import "forge-std/Test.sol";
import "forge-std/console.sol";
import "./common/SafeguardBaseTest.sol";
import {GMXV1SolvSafeguard} from "../src/GMXV1SolvSafeguard.sol";

contract GMXV1SolvSafeguardTest is SafeguardBaseTest {

	function setUp() public override {
		super.setUp();
		_createGMXV1SolvSafeguard();
	}

	function test_GMXV1_E2E_SUCCESS() public {
		//initArgus
		bytes memory data = hex"";
		_callExecTransaction(SAFE_MULTI_SEND_CONTRACT, data, Enum.Operation.DelegateCall);

		//addRoles
		data = hex"";
		_callExecTransaction(SAFE_MULTI_SEND_CONTRACT, data, Enum.Operation.DelegateCall);

		//GrantRoles
		data = hex"";
		_callExecTransaction(SAFE_MULTI_SEND_CONTRACT, data, Enum.Operation.DelegateCall);

		//AddGmxAuthorizer
		data = hex"";
		_callExecTransaction(SAFE_MULTI_SEND_CONTRACT, data, Enum.Operation.DelegateCall);

		//cex recharge
		data = abi.encodeWithSelector(bytes4(keccak256("transfer(address,uint256)")), CEX_RECHARGE_ADDRESS, 1000);
		_callExecTransaction(USDT, data, Enum.Operation.Call);

	}

	function _createGMXV1SolvSafeguard() internal returns (GMXV1SolvSafeguard) {
		GMXV1SolvSafeguard gmxV1SolvSafeguard = new GMXV1SolvSafeguard(
			SAFE_ACCOUNT,
			USDT,
			CEX_RECHARGE_ADDRESS,
			OPEN_END_FUND_MARKET,
			OPEN_END_FUND_SHARE,
			OPEN_END_FUND_REDEMPTION
		);
		_setSafeGuard(address(gmxV1SolvSafeguard));
		return gmxV1SolvSafeguard;
	}
}